<?xml version="1.0" encoding="UTF-8"?>

<diagram xmlns="https://prefigure.org" width="225" height="225" margins="10">
    <definition>
    alignments=['nw','nw','nw','nw','w','sw','sw','sw','sw','s','se','se','se','se','e','ne','ne','ne','ne','n']
    </definition>
    <definition>f(t)=(-sin(2*pi*(1+t)/20),cos(2*pi*(1+t)/20))</definition>
    <coordinates bbox="[-1.4,-1.4,1.4,1.4]">
    <repeat parameter="k=0..19">
        <point at="point" p="f(k)" alignment="alignments[k]" size="2">
        <m>${20-k}</m>
        </point>
    </repeat>
    </coordinates>
