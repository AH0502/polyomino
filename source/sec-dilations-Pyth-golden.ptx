            </shortdescription>
          </image>
        </figure>
      </introduction>
      <task>
        <statement>
          <p>
            <m>m\angle FGH</m>
          </p>
        </statement>
      </task>
      <task>
        <statement>
          <p>
            <m>m\angle FHG</m>
          </p>
        </statement>
      </task>
      <task>
        <statement>
          <p>
            <m>m\angle JIK</m>
          </p>
        </statement>
      </task>
      <task>
        <statement>
          <p>
            <m>m\angle KIJ</m>
          </p>
